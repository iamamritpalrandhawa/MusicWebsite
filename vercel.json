{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/id/:youtubeId",
      "dest": "/index.js"
    },
    {
      "src": "/name/:youtubename",
      "dest": "/index.js"
    },
    {
      "src": "/getalbums/:albumname",
      "dest": "/index.js"
    },
    {
      "src": "/albumid/:albumId",
      "dest": "/index.js"
    },
    {
      "src": "/",
      "handle": "build/index.html"
    },
    {
      "src": "/(.*)",
      "handle": "build/$1"
    }
  ]
}
