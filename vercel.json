{
  "version": 2,
  "routes": [
    {
      "src": "/id/:youtubeId",
      "dest": "/index.js"
    },
    {
      "src": "/name/:youtubename",
      "dest": "/index.js"
    },
    {
      "src": "/getalbums/:albumname",
      "dest": "/index.js"
    },
    {
      "src": "/albumid/:albumId",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "build/index.html"
    }
  ]
}
