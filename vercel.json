{
  "version": 2,
  "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "build/**",
            "use": "@vercel/static"
        }
    ],
  "routes": [
    {
      "src": "/id/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/name/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/getalbums/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/albumid/(.*)",
      "dest": "/index.js"
    },
    {
       "src": "/",
      "build": "build/index.html"
    }
        
  ]
}
